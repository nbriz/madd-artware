{
  "name": "Confetti",
  "credit": "Tiffany",
  "type": "tool",
  "function": "let random = (min, max) => min + Math.random() * (max-min)\nlet r = random(0,255)\nlet g = random(0,255)\nlet b = random(0,255)\n\n\nif (canvas.mouseDown) {\n  canvas.fillColor = rgba(r,g,b,0.5)\n  canvas.strokeColor = canvas.fillColor\n  \n  let x = canvas.mouseX\n  let y = canvas.mouseY\n  let radius = Math.random() * 100\n  canvas.circle(x,y,radius)\n} else {\n  canvas.fillColor = rgba(r,g,b,0.5)\n  canvas.strokeColor = 'white'\n}",
  "icon": "data:image/png;base64,",
  "click": false
}
