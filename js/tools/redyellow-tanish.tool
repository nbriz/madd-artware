{
  "name": "redyellow",
  "type": "tool",
  "function": "let x = canvas.mouseX\nlet y = canvas.mouseY\nlet radius = 20\n\nlet i = 0\nlet count = 8\n\nwhile (i < count) {\n  let angle = Math.PI * 2 * i / count\n  let cx = x + Math.cos(angle) * 40\n  let cy = y + Math.sin(angle) * 40\n\n  if (i % 2 === 0) {\n    canvas.fillColor = 'red'\n  } else {\n    canvas.fillColor = 'yellow'\n  }\n\n  canvas.circle(cx, cy, radius)\n  i++\n}\n",
  "icon": "data:image/png;base64,",
  "click": false
}