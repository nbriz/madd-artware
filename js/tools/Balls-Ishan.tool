{
  "name": "balls",
  "type": "tool",
  "function": "(() => {\n  const oldCanvas = document.querySelector('canvas')\n  const canvas = oldCanvas.cloneNode()\n  oldCanvas.parentNode.replaceChild(canvas, oldCanvas)\n  const ctx = canvas.getContext('2d')\n  const circles = []\n  canvas.addEventListener('click', event => {\n    const rect   = canvas.getBoundingClientRect()\n    const [startX,startY] = [event.clientX-rect.left, event.clientY-rect.top]\n    const [r,g,b] = [Math.floor(Math.random()*256), Math.floor(Math.random()*256), Math.floor(Math.random()*256)]\n    const color = `rgb(${r}, ${g}, ${b})`\n    for (let i = 0; i < 5; i++) {\n      circles.push({x: startX, y: startY, speedx: Math.random()*10,speedy: Math.random()*10, r:5, color})\n    }\n  });\n  function animate() {\n    ctx.clearRect(0, 0, canvas.width, canvas.height)\n    for (let i = 0; i <= circles.length-1; i++) {\n      const circ = circles[i]\n      circ.x+=circ.speedx\n      circ.y+=circ.speedy\n      if (circ.x<0 || circ.x>canvas.width)  \n        circ.speedx*=-1\n      if (circ.y<0 || circ.y>canvas.height)\n        circ.speedy*=-1\n      ctx.beginPath()\n      ctx.arc(circ.x, circ.y, circ.r, 0, Math.PI * 2)\n      ctx.fillStyle = circ.color\n      ctx.fill()\n      circ.r -= 0.01\n    }\n    requestAnimationFrame(animate)\n  }\n  animate()\n})();",
  "icon": "data:image/png;base64,",
  "click": false
}