{
  "name": "pencil",
  "type": "tool",
  "click": false,
  "icon": "data:image/png;base64,",
  "function": "if (canvas.mouseDown) {\n  canvas.line(canvas.mouseX, canvas.mouseY, canvas.prevX, canvas.prevY)\n  canvas.prevX = canvas.mouseX\n  canvas.prevY = canvas.mouseY\n} else {\n  canvas.prevX = canvas.mouseX\n  canvas.prevY = canvas.mouseY\n}"
}
