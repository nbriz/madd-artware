{
  "name": "pink hearts",
  "credit": "Maryam",
  "type": "tool",
  "function": "if (canvas.mouseDown) {\n  let x = canvas.mouseX\n  let y = canvas.mouseY\n  let size = 40\n\n  canvas.fillColor = 'pink'\n\n  let topRadius = size / 2\n\n  canvas.circle(x - topRadius / 2, y - topRadius / 2, topRadius)\n\n  canvas.circle(x + topRadius / 2, y - topRadius / 2, topRadius)\n\n  canvas.triangle(\n    x - size, y,\n    x + size, y,\n    x, y + size * 1.5\n  )\n}\n",
  "icon": "data:image/png;base64,",
  "click": false
}
